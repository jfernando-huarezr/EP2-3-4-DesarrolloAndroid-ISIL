<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <!-- Always include this permission -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- Include only if your app benefits from precise location access. -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EP2"
        android:usesCleartextTraffic="true"
        tools:targetApi="34">
        <activity
            android:name=".DetalleEnvioActivity"
            android:exported="false"
            android:label="@string/title_activity_detalle_envio"
            android:theme="@style/Theme.EP2" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value= "${googleMapsKey}" />

        <activity
            android:name=".EnviosActivity"
            android:exported="false"
            android:label="@string/title_activity_envios"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".TopTenActivity"
            android:exported="false"
            android:label="@string/title_activity_top_ten"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".CajaInsertActivity"
            android:exported="false"
            android:label="@string/title_activity_caja_insert"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".CajaActivity"
            android:exported="false"
            android:label="@string/title_activity_caja"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".VideoActivity"
            android:exported="false"
            android:label="@string/title_activity_video"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:label="@string/title_activity_splash"
            android:theme="@style/Theme.EP2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetallePedidoActivity"
            android:exported="false"
            android:label="@string/title_activity_detalle_pedido"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".PedidosActivity"
            android:exported="false"
            android:label="@string/title_activity_pedidos"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.EP2" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="Anime"
            android:theme="@style/Theme.EP2" />
    </application>

</manifest>